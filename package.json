{
  "name": "gml-script-wizard",
  "version": "0.9.1",
  "description": "GML Script Wizard",
  "main": "index.js",
  "repository": "https://github.com/mstop4/gml-script-wizard.git",
  "author": "mstop4 <mstop4@gmail.com>",
  "license": "BSD-2-Clause",
  "scripts": {
    "build": "webpack-dev-server --port 8080 --host 0.0.0.0 --hot --open --config webpack.dev.js",
    "build:prod": "./node_modules/.bin/webpack --config webpack.prod.js",
    "test": "mocha --reporter spec ./test/index.js \"./test/components/*.spec.js\"",
    "lint": "esw webpack.config.* src tools --color",
    "lint:watch": "yarn run lint --watch",
    "electron:dev": "ELECTRON_START_URL=http://localhost:8080 electron ./src/js/electron-index.js",
    "electron:prod": "yarn build && electron ./src/js/electron-index.js",
    "start": "npm-run-all --parallel build lint:watch",
    "gh-pages": "yarn build:prod && push-dir --dir=dist --branch=gh-pages --cleanup"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "chai": "^4.1.2",
    "clean-webpack-plugin": "^0.1.17",
    "css-loader": "^0.28.9",
    "electron": "^2.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.18.0",
    "eslint-plugin-react": "^7.6.1",
    "eslint-watch": "^3.1.3",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.0.6",
    "inline-source-map": "^0.6.2",
    "jsdom": "^11.6.0",
    "mocha": "^5.0.0",
    "npm-run-all": "^4.1.2",
    "path": "^0.12.7",
    "react-addons-test-utils": "^15.6.2",
    "style-loader": "^0.20.2",
    "uglifyjs-webpack-plugin": "^1.1.6",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.11.1",
    "webpack-merge": "^4.1.1"
  },
  "dependencies": {
    "autosuggest-highlight": "^3.1.1",
    "babel-polyfill": "^6.26.0",
    "lodash": "^4.17.5",
    "material-ui": "^1.0.0-beta.36",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-autosuggest": "^9.3.3",
    "react-custom-scrollbars": "^4.2.1",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.7",
    "react-sortable-hoc": "^0.6.8",
    "redux": "^3.7.2"
  }
}
