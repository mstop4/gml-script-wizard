{
  "name": "gml-script-wizard",
  "version": "0.9.3",
  "description": "GML Script Wizard",
  "main": "index.js",
  "repository": "https://github.com/mstop4/gml-script-wizard.git",
  "author": "mstop4 <mstop4@gmail.com>",
  "license": "BSD-2-Clause",
  "scripts": {
    "start": "webpack-dev-server --port 8081 --host 0.0.0.0 --hot --open --config webpack.dev.js",
    "build": "./node_modules/.bin/webpack --config webpack.prod.js && cp -r dist/js electron && cp -r dist/index.html electron",
    "lint": "esw webpack.config.* src tools --color",
    "lint:watch": "yarn lint --watch",
    "electron": "npx electron electron/main.js",
    "build:electron-win": "electron-packager electron GMLScriptWizard --platform=win32 --arch=x64 --overwrite --no-tmpdir",
    "build:electron-mac": "electron-packager electron GMLScriptWizard --platform=darwin --arch=x64 --overwrite --no-tmpdir",
    "build:electron-linux": "electron-packager electron GMLScriptWizard --platform=linux --arch=x64 --overwrite --no-tmpdir",
    "gh-pages": "yarn build && npx push-dir --dir=dist --branch=gh-pages --cleanup"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "clean-webpack-plugin": "^0.1.17",
    "css-loader": "^0.28.9",
    "electron": "^15.5.5",
    "electron-packager": "^13.0.1",
    "eslint": "^4.18.0",
    "eslint-plugin-react": "^7.6.1",
    "eslint-watch": "^3.1.3",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.0.6",
    "inline-source-map": "^0.6.2",
    "path": "^0.12.7",
    "style-loader": "^0.20.2",
    "uglifyjs-webpack-plugin": "^1.1.6",
    "webpack": "^4.28.3",
    "webpack-cli": "^3.2.0",
    "webpack-dev-server": "^3.1.14",
    "webpack-merge": "^4.1.1"
  },
  "dependencies": {
    "autosuggest-highlight": "^3.1.1",
    "babel-polyfill": "^6.26.0",
    "lodash": "^4.17.13",
    "material-ui": "^1.0.0-beta.36",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-autosuggest": "^9.3.3",
    "react-custom-scrollbars": "^4.2.1",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.7",
    "react-sortable-hoc": "^0.6.8",
    "redux": "^3.7.2"
  }
}
